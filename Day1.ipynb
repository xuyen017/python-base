    "def landmark(height, left = 'left', right='right', middle='middle'):\n",
    "    if height:\n",
    "        landmark(height-1, left, middle, right)\n",
    "        print(left, '=>', right)\n",
    "        landmark(height-1, middle, right, left)\n"
   ]
